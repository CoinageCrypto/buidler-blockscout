{
	"name": "buidler-blockscout",
	"version": "0.0.1",
	"description": "Buidler TypeScript plugin for verifying contracts on blockscout",
	"repository": "github:CoinageCrypto/buidler-blockscout",
	"contributors": [
		"Nomic Labs SRL",
		"Marin PetruniÄ‡ <marin@nodefactory.io>",
